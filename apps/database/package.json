{
  "name": "@gestao/database",
  "version": "0.1.0",
  "private": true,
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc -p tsconfig.json",
    "lint": "eslint src --ext .ts",
    "migrate": "dotenv -e ../../.env -- prisma migrate deploy",
    "migrate:dev": "dotenv -e ../../.env -- prisma migrate dev",
    "seed": "dotenv -e ../../.env -- ts-node src/seed.ts",
    "generate": "dotenv -e ../../.env -- prisma generate"
  },
  "dependencies": {
    "@prisma/client": "^5.19.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.30",
    "prisma": "^5.19.0",
    "ts-node": "^10.9.1",
    "typescript": "^5.4.0"
  }
}
