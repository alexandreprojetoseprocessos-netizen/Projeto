{
  "name": "gestao-projetos",
  "private": true,
  "workspaces": [
    "apps/api",
    "apps/front",
    "apps/database"
  ],
  "scripts": {
    "dev:api": "npm --workspace apps/api run dev",
    "dev:front": "npm --workspace apps/front run dev",
    "db:migrate": "npm --workspace apps/database run migrate",
    "db:seed": "npm --workspace apps/database run seed",
    "lint": "npm --workspaces run lint"
  },
  "devDependencies": {
    "dotenv-cli": "^7.3.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "@typescript-eslint/parser": "^7.8.0",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "typescript": "^5.4.0"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
